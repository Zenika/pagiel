MONGO_INITDB_ROOT_USERNAME=root
MONGO_INITDB_ROOT_PASSWORD=root

# InfluxDB
INFLUXDB_HOST=influxdb
INFLUXDB_PORT=8086
INFLUXDB_RETENTION=24w

INFLUXDB_ORG_NAME=default
INFLUXDB_USERNAME=default
